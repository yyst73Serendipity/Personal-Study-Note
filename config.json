{
  "theme": {
    "primaryColor": "#4dd0e1",
    "backgroundColor": "#1A1E24",
    "cardBackgroundColor": "#2C323A",
    "textColor": "#E0E0E0",
    "secondaryTextColor": "#B0B0B0",
    "borderColor": "#3F6E7A",
    "accentColor": "#52C41A"
  },
  "ai": {
    "provider": "deepseek",
    "model": "deepseek-chat",
    "apiEndpoint": "https://api.deepseek.com/v1/chat/completions",
    "maxTokens": 500,
    "temperature": 0.7,
    "welcomeMessage": "你好！我是{name}的AI助手小易，有什么可以帮助你的吗？😊"
  },
  "security": {
    "editTokenHash": "",
    "allowedOrigins": [
      "http://localhost:8080",
      "http://127.0.0.1:8080",
      "http://localhost:8000",
      "http://127.0.0.1:8000"
    ],
    "rateLimitPerMinute": 20
  },
  "site": {
    "title": "{name}的个人网站",
    "description": "AI产品经理个人简历展示网站",
    "keywords": [
      "AI产品经理",
      "简历",
      "技能展示",
      "个人网站"
    ],
    "analytics": {
      "enabled": false,
      "googleAnalyticsId": ""
    }
  },
  "features": {
    "editMode": true,
    "aiAssistant": true,
    "backup": true
  }
}